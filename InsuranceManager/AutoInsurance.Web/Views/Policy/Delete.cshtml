@model Autoinsurance.Models.Policies

<h2>Eliminar Póliza</h2>
<p>¿Estás seguro de que deseas eliminar la póliza con número @Model.PolicyNumber?</p>
@using (Html.BeginForm())
{
    @Html.HiddenFor(m => m.Id)
    <button type="submit">Eliminar</button>
}
<a href="@Url.Action("Index", "Policies")">Cancelar</a>
